{
  "name": "alex-r-bigelow-gh-pages",
  "version": "0.1.0",
  "description": "Personal site for Alex Bigelow",
  "repository": {
    "type": "git"
  },
  "author": "Alex Bigelow",
  "dependencies": {
    "@ukijs/ui": "^0.2.5",
    "d3": "^7.9.0",
    "less": "^4.3.0",
    "uki": "^0.7.9"
  },
  "scripts": {
    "lint": "eslint '**/*.js' --quiet",
    "lintFix": "eslint **/*.js' --fix",
    "serve": "serve .",
    "build": "node ./build.js"
  },
  "eslintConfig": {
    "extends": "standard",
    "rules": {
      "semi": [
        2,
        "always"
      ],
      "no-extra-semi": 2,
      "semi-spacing": [
        2,
        {
          "before": false,
          "after": true
        }
      ],
      "space-before-function-paren": "off"
    },
    "globals": {
      "globalThis": false
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none"
  },
  "devDependencies": {
    "bibtex-parse-js": "^0.0.23",
    "eslint": "^9.24.0",
    "eslint-config-semistandard": "^17.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "gray-matter": "^4.0.3",
    "serve": "^14.2.4",
    "shelljs": "^0.9.2",
    "showdown": "^2.1.0"
  }
}
