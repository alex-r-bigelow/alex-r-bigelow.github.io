{
  "name": "alex-r-bigelow-gh-pages",
  "version": "0.1.0",
  "description": "Personal site for Alex Bigelow",
  "repository": {
    "type": "git"
  },
  "author": "Alex Bigelow",
  "dependencies": {
    "@ukijs/ui": "^0.2.4",
    "d3": "^6.3.0",
    "less": "^3.12.2",
    "uki": "^0.7.6"
  },
  "scripts": {
    "lint": "eslint '**/*.js' --quiet",
    "lintFix": "eslint **/*.js' --fix",
    "serve": "serve .",
    "build": "node ./build.js"
  },
  "eslintConfig": {
    "extends": "standard",
    "rules": {
      "semi": [
        2,
        "always"
      ],
      "no-extra-semi": 2,
      "semi-spacing": [
        2,
        {
          "before": false,
          "after": true
        }
      ]
    },
    "globals": {
      "globalThis": false
    }
  },
  "devDependencies": {
    "bibtex-parse-js": "0.0.24",
    "eslint": "^7.14.0",
    "eslint-config-semistandard": "^15.0.1",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "ga-lite": "^2.1.0",
    "gray-matter": "^4.0.2",
    "serve": "^11.3.2",
    "shelljs": "^0.8.4",
    "showdown": "^1.9.1"
  }
}
